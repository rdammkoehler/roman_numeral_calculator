char result[100];

#test one_plus_one_is_two
	add(result, "I", "I");
	ck_assert_str_eq(result, "II");
#test one_plus_two_is_three
	add(result, "II", "I");
	ck_assert_str_eq(result, "III");
#test two_plus_two_is_four
	add(result, "II", "II");
	ck_assert_str_eq(result, "IV");
#test two_plus_three_is_five
	add(result, "II", "III");
	ck_assert_str_eq(result, "V");
#test three_plus_three_is_six
	add(result, "III", "III");
	ck_assert_str_eq(result, "VI");
#test four_plus_one_is_five
	add(result, "IV", "I");
	ck_assert_str_eq(result, "V");
#test one_plus_four_is_five
	add(result, "I", "IV");
	ck_assert_str_eq(result, "V");
#test five_plus_four_is_nine
	add(result, "V", "IV");
	ck_assert_str_eq(result, "IX");
#test four_plus_five_is_nine
	add(result, "IV", "V");
	ck_assert_str_eq(result, "IX");
#test six_and_three_is_nine
	add(result, "VI", "III");
	ck_assert_str_eq(result, "IX");
#test three_and_six_is_nine
	add(result, "III", "VI");
	ck_assert_str_eq(result, "IX");
#test seven_and_two_is_nine
	add(result, "VII", "II");
	ck_assert_str_eq(result, "IX");
#test two_and_seven_is_nine
	add(result, "II", "VII");
	ck_assert_str_eq(result, "IX");
#test eight_and_one_is_nine
	add(result, "VIII", "I");
	ck_assert_str_eq(result, "IX");
#test one_and_eight_is_nine
	add(result, "I", "VIII");
	ck_assert_str_eq(result, "IX");
